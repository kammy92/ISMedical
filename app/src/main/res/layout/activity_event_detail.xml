<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    android:id="@+id/clMain"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:animateLayoutChanges="true"
    android:background="#ffffff">
    <android.support.design.widget.AppBarLayout
        android:id="@+id/appBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:animateLayoutChanges="true"
        android:theme="@style/AppTheme.AppBarOverlay"
        android:visibility="visible"
        app:elevation="0dp">
        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar1"
            style="@style/Widget.MyApp.ActionBar"
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:animateLayoutChanges="true"
            android:background="#ffffff"
            android:theme="@style/Base.ThemeOverlay.AppCompat.Dark"
            app:contentInsetEnd="0dp"
            app:contentInsetLeft="0dp"
            app:contentInsetRight="0dp"
            app:contentInsetStart="0dp"
            app:layout_collapseMode="parallax">

            <RelativeLayout
                android:id="@+id/toolbar_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:id="@+id/rlBack"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">
                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:layout_margin="@dimen/activity_margin"
                        android:src="@drawable/ic_arrow_back"
                        android:textColor="@color/text_color_grey_dark2"
                        android:visibility="visible"/>
                </RelativeLayout>
                <TextView
                    android:id="@+id/tvTitleEventName"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:ellipsize="end"
                    android:lines="1"
                    android:maxLines="1"
                    android:text="EVENT DETAILS"
                    android:textColor="@color/primary_text2"
                    android:textSize="@dimen/text_size_medium"
                    android:textStyle="bold"/>
            </RelativeLayout>
        </android.support.v7.widget.Toolbar>
        <View
            android:id="@+id/v1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:background="@color/text_color_grey_light2"
            android:visibility="visible"/>
    </android.support.design.widget.AppBarLayout>
    <android.support.v4.widget.NestedScrollView
        android:id="@+id/nestedScrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/appBar"
        android:animateLayoutChanges="true"
        android:descendantFocusability="blocksDescendants"
        android:overScrollMode="never"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">
        <RelativeLayout
            android:id="@+id/rlMain"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:animateLayoutChanges="true"
            android:visibility="gone">
            <RelativeLayout
                android:id="@+id/rl22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clipToPadding="false"
                android:orientation="vertical">
                <RelativeLayout
                    android:id="@+id/rl2"
                    android:layout_width="match_parent"
                    android:layout_height="120dp">
                    <ImageView
                        android:id="@+id/ivEventImage"
                        android:layout_width="match_parent"
                        android:layout_height="120dp"
                        android:scaleType="fitXY"/>
                    <ProgressBar
                        android:id="@+id/progressBar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:visibility="visible"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/rl2">
                    <TextView
                        android:id="@+id/tvEventDates"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:paddingBottom="@dimen/activity_margin_extra_small"
                        android:paddingLeft="@dimen/activity_margin_small"
                        android:paddingRight="@dimen/activity_margin_small"
                        android:paddingTop="@dimen/activity_margin_extra_small"
                        android:text="10 Feb - 12 Feb"
                        android:textColor="#A20057"
                        android:textSize="@dimen/text_size_extra_small"/>
                    <TextView
                        android:id="@+id/tvEventName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/tvEventDates"
                        android:paddingLeft="@dimen/activity_margin_small"
                        android:paddingRight="@dimen/activity_margin_small"
                        android:text="EXPODENT"
                        android:textColor="@color/primary_text2"
                        android:textSize="@dimen/text_size_small"
                        android:textStyle="bold"/>
                    <TextView
                        android:id="@+id/tvEventVenue"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/tvEventName"
                        android:paddingBottom="@dimen/activity_margin_extra_small"
                        android:paddingLeft="@dimen/activity_margin_small"
                        android:paddingRight="@dimen/activity_margin_small"
                        android:paddingTop="@dimen/activity_margin_extra_small"
                        android:text="Pragati Maidan, New Delhi"
                        android:textColor="@color/secondary_text2"
                        android:textSize="@dimen/text_size_extra_small"/>
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rl22"
                android:layout_marginTop="@dimen/activity_margin"
                android:animateLayoutChanges="true">
                <RelativeLayout
                    android:id="@+id/rlSchedule"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:animateLayoutChanges="true">
                    <RelativeLayout
                        android:id="@+id/rlE1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@color/tertiary_text2"/>
                        <ImageView
                            android:id="@+id/iv1"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/activity_margin_small"
                            android:layout_marginStart="@dimen/activity_margin_small"
                            android:src="@drawable/ic_event_schedule"/>
                        <TextView
                            android:id="@+id/tv1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/iv1"
                            android:layout_toRightOf="@+id/iv1"
                            android:padding="@dimen/activity_margin_small"
                            android:text="SCHEDULE"
                            android:textColor="@color/primary_text2"
                            android:textSize="@dimen/text_size_medium"
                            android:textStyle="bold"/>
                        <ImageView
                            android:id="@+id/ivE1"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_margin="@dimen/activity_margin_small"
                            android:src="@drawable/ic_arrow_down"/>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_below="@+id/tv1"
                            android:background="@color/tertiary_text2"/>
                    </RelativeLayout>
                    <android.support.v7.widget.RecyclerView
                        android:id="@+id/rvSchedule"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/rlE1"
                        android:clipToPadding="false"
                        android:fillViewport="true"
                        android:overScrollMode="never"
                        android:scrollbarStyle="insideOverlay"
                        android:visibility="gone"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rlSpeakers"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rlSchedule"
                    android:layout_marginTop="@dimen/activity_margin_small"
                    android:animateLayoutChanges="true">
                    <RelativeLayout
                        android:id="@+id/rlE2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@color/tertiary_text2"/>
                        <ImageView
                            android:id="@+id/iv2"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/activity_margin_small"
                            android:layout_marginStart="@dimen/activity_margin_small"
                            android:src="@drawable/ic_event_speaker"/>
                        <TextView
                            android:id="@+id/tv2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/iv2"
                            android:layout_toRightOf="@+id/iv2"
                            android:padding="@dimen/activity_margin_small"
                            android:text="SPEAKERS"
                            android:textColor="@color/primary_text2"
                            android:textSize="@dimen/text_size_medium"
                            android:textStyle="bold"/>
                        <ImageView
                            android:id="@+id/ivE2"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_margin="@dimen/activity_margin_small"
                            android:src="@drawable/ic_arrow_down"/>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_below="@+id/tv2"
                            android:background="@color/tertiary_text2"/>
                    </RelativeLayout>
                    <android.support.v7.widget.RecyclerView
                        android:id="@+id/rvSpeakers"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/rlE2"
                        android:clipToPadding="false"
                        android:fillViewport="true"
                        android:overScrollMode="never"
                        android:scrollbarStyle="insideOverlay"
                        android:visibility="gone"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rlExhibitors"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/rlSpeakers"
                    android:layout_marginTop="@dimen/activity_margin_small"
                    android:animateLayoutChanges="true">
                    <RelativeLayout
                        android:id="@+id/rlE3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@color/tertiary_text2"/>
                        <ImageView
                            android:id="@+id/iv3"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/activity_margin_small"
                            android:layout_marginStart="@dimen/activity_margin_small"
                            android:src="@drawable/ic_event_exhibitor"/>
                        <TextView
                            android:id="@+id/tv3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/iv3"
                            android:layout_toRightOf="@+id/iv3"
                            android:padding="@dimen/activity_margin_small"
                            android:text="EXHIBITORS"
                            android:textColor="@color/primary_text2"
                            android:textSize="@dimen/text_size_medium"
                            android:textStyle="bold"/>
                        <ImageView
                            android:id="@+id/ivE3"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_margin="@dimen/activity_margin_small"
                            android:src="@drawable/ic_arrow_down"/>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_below="@+id/tv3"
                            android:background="@color/tertiary_text2"/>
                    </RelativeLayout>
                    <android.support.v7.widget.RecyclerView
                        android:id="@+id/rvExhibitors"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/rlE3"
                        android:clipToPadding="false"
                        android:fillViewport="true"
                        android:overScrollMode="never"
                        android:scrollbarStyle="insideOverlay"
                        android:visibility="gone"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rlFloorPlan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/rlExhibitors"
                    android:layout_marginTop="@dimen/activity_margin_small"
                    android:animateLayoutChanges="true">
                    <RelativeLayout
                        android:id="@+id/rlE4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@color/tertiary_text2"/>
                        <ImageView
                            android:id="@+id/iv4"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/activity_margin_small"
                            android:layout_marginStart="@dimen/activity_margin_small"
                            android:src="@drawable/ic_event_floor_plan"/>
                        <TextView
                            android:id="@+id/tv4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/iv4"
                            android:layout_toRightOf="@+id/iv4"
                            android:padding="@dimen/activity_margin_small"
                            android:text="FLOOR PLAN"
                            android:textColor="@color/primary_text2"
                            android:textSize="@dimen/text_size_medium"
                            android:textStyle="bold"/>
                        <ImageView
                            android:id="@+id/ivE4"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_margin="@dimen/activity_margin_small"
                            android:src="@drawable/ic_arrow_down"/>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_below="@+id/tv4"
                            android:background="@color/tertiary_text2"/>
                    </RelativeLayout>
                    <com.davemorrissey.labs.subscaleview.SubsamplingScaleImageView
                        android:id="@+id/ivFloorPlan"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@+id/rlE4"
                        android:layout_margin="@dimen/activity_margin"
                        android:visibility="gone"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rlInformation"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/rlFloorPlan"
                    android:layout_marginTop="@dimen/activity_margin_small"
                    android:animateLayoutChanges="true">
                    <RelativeLayout
                        android:id="@+id/rlE5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@color/tertiary_text2"/>
                        <ImageView
                            android:id="@+id/iv5"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/activity_margin_small"
                            android:layout_marginStart="@dimen/activity_margin_small"
                            android:src="@drawable/ic_event_general_info"/>
                        <TextView
                            android:id="@+id/tv5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/iv5"
                            android:layout_toRightOf="@+id/iv5"
                            android:padding="@dimen/activity_margin_small"
                            android:text="INFORMATION"
                            android:textColor="@color/primary_text2"
                            android:textSize="@dimen/text_size_medium"
                            android:textStyle="bold"/>
                        <ImageView
                            android:id="@+id/ivE5"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_margin="@dimen/activity_margin_small"
                            android:src="@drawable/ic_arrow_down"/>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_below="@+id/tv5"
                            android:background="@color/tertiary_text2"/>
                    </RelativeLayout>
                    <WebView
                        android:id="@+id/wvInformation"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/rlE5"
                        android:visibility="gone"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rlRegistration"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/rlInformation"
                    android:layout_marginTop="@dimen/activity_margin_small"
                    android:animateLayoutChanges="true">
                    <RelativeLayout
                        android:id="@+id/rlE6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="@color/tertiary_text2"/>
                        <ImageView
                            android:id="@+id/iv6"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/activity_margin_small"
                            android:layout_marginStart="@dimen/activity_margin_small"
                            android:src="@drawable/ic_event_registration"/>
                        <TextView
                            android:id="@+id/tv6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/iv6"
                            android:layout_toRightOf="@+id/iv6"
                            android:padding="@dimen/activity_margin_small"
                            android:text="REGISTRATION"
                            android:textColor="@color/primary_text2"
                            android:textSize="@dimen/text_size_medium"
                            android:textStyle="bold"/>
                        <ImageView
                            android:id="@+id/ivE6"
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:layout_alignParentEnd="true"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_margin="@dimen/activity_margin_small"
                            android:src="@drawable/ic_arrow_down"/>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_below="@+id/tv6"
                            android:background="@color/tertiary_text2"/>
                    </RelativeLayout>
                    <WebView
                        android:id="@+id/wvRegistration"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/rlE6"
                        android:visibility="gone"/>
                </RelativeLayout>
            </RelativeLayout>
        </RelativeLayout>
    </android.support.v4.widget.NestedScrollView>
    <ProgressBar
        android:id="@+id/progressBar2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:visibility="visible"/>
    <RelativeLayout
        android:id="@+id/rlNoResult"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:animateLayoutChanges="true"
        android:visibility="gone"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">
        <ImageView
            android:id="@+id/ivNoResult"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:src="@drawable/ic_no_internet"/>
        <TextView
            android:id="@+id/tvNoResultTitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ivNoResult"
            android:layout_marginTop="@dimen/activity_margin"
            android:gravity="center"
            android:padding="@dimen/edittext_padding"
            android:textColor="@color/primary_text2"
            android:textSize="@dimen/text_size_extra_large"
            android:textStyle="bold"
            android:visibility="visible"/>
        <TextView
            android:id="@+id/tvNoResultDescription"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tvNoResultTitle"
            android:gravity="center"
            android:paddingLeft="@dimen/activity_margin"
            android:paddingRight="@dimen/activity_margin"
            android:textColor="@color/secondary_text2"
            android:textSize="@dimen/text_size_medium"/>
        <TextView
            android:id="@+id/tvNoResultButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tvNoResultDescription"
            android:gravity="center"
            android:padding="@dimen/activity_margin"
            android:text="RETRY"
            android:textColor="@color/accent2"
            android:textSize="@dimen/text_size_extra_large"
            android:textStyle="bold"/>
    </RelativeLayout>
    <com.indiasupply.ismedical.utils.ShimmerFrameLayout
        android:id="@+id/shimmer_view_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <RelativeLayout
                android:id="@+id/rll1"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize">
                <View
                    android:id="@+id/ivl1"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:layout_margin="@dimen/activity_margin"
                    android:visibility="invisible"/>
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toEndOf="@+id/ivl1"
                    android:layout_toRightOf="@+id/ivl1">
                    <TextView
                        android:id="@+id/tvView1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@color/shimmer_bg_color"
                        android:ellipsize="end"
                        android:lines="1"
                        android:maxLines="1"
                        android:text="                      "
                        android:textColor="@color/primary_text2"
                        android:textSize="@dimen/text_size_medium"
                        android:textStyle="bold"
                        android:visibility="gone"/>
                    <TextView
                        android:id="@+id/tvView2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tvView1"
                        android:layout_marginTop="@dimen/activity_margin_extra_small"
                        android:background="@color/shimmer_bg_color"
                        android:ellipsize="end"
                        android:lines="1"
                        android:maxLines="1"
                        android:text="                                     "
                        android:textColor="@color/secondary_text2"
                        android:textSize="@dimen/text_size_small"
                        android:visibility="gone"/>
                </RelativeLayout>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rlLoading"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/rll1">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/activity_margin"
                        android:layout_marginRight="@dimen/activity_margin"
                        android:layout_marginTop="@dimen/activity_margin"
                        android:orientation="horizontal"
                        android:weightSum="2">
                        <RelativeLayout
                            xmlns:android="http://schemas.android.com/apk/res/android"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/activity_margin_small"
                            android:layout_weight="1"
                            android:background="@color/shimmer_bg_color"
                            android:orientation="vertical">
                            <RelativeLayout
                                android:id="@+id/rl1"
                                android:layout_width="50dp"
